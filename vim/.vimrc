set nocompatible

filetype on
filetype plugin on 
filetype indent on

syntax on

set number
set shiftwidth=4
set tabstop=4
set expandtab
set nobackup
set incsearch
set ignorecase
set smartcase
set showcmd
set showmode
set showmatch
set hlsearch
set wildmenu
set wildmode=list:longest
set wildignore=*.jpg

let &t_SI = "\<Esc>[5 q"
let &t_SR = "\<Esc>[5 q"
let &t_EI = "\<Esc>[1 q"

inoremap jk <esc>


